<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="Boletim" targetNamespace="http://tempuri.org/Boletim.xsd" elementFormDefault="qualified" xmlns="http://tempuri.org/Boletim.xsd" xmlns:mstns="http://tempuri.org/Boletim.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="OrientacaoEducacional">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Ativos" type="xs:integer" />
        <xs:element name="Transferidos" type="xs:integer" />
        <xs:element name="Remanejados" type="xs:integer" />
        <xs:element name="Aprovados" type="xs:integer" />
        <xs:element name="Abandonos" type="xs:integer" />
